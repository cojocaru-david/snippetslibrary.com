{
  "name": "snippetslibrary.com",
  "version": "0.1.11",
  "private": true,
  "description": "A modern, full-stack code snippet manager for developers",
  "author": "Cojocaru David",
  "overrides": {
    "esbuild": "^0.25.8"
  },
  "license": "MIT",
  "homepage": "https://www.snippetslibrary.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cojocaru-david/snippetslibrary.com"
  },
  "keywords": [
    "snippets",
    "code",
    "library",
    "nextjs",
    "react",
    "drizzle",
    "tailwind",
    "fullstack"
  ],
  "scripts": {
    "dev": "prettier --write . && next dev --turbopack --port 3001",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start --port 3001",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate && drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "migrate": "npm run db:generate && npm run db:migrate",
    "format": "prettier --write ./src"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@types/aos": "^3.0.7",
    "@types/uuid": "^10.0.0",
    "aos": "^2.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.44.5",
    "gsap": "^3.13.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.12",
    "nanoid": "^5.1.5",
    "next": "15.5.3",
    "next-auth": "^5.0.0-beta.29",
    "pg": "^8.16.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prettier": "^3.6.2",
    "esbuild": "^0.25.8",
    "shiki": "^3.12.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
