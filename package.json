{
  "name": "snippetslibrary.com",
  "version": "0.1.2",
  "private": true,
  "description": "A modern, full-stack code snippet manager for developers",
  "author": "Cojocaru David",
  "license": "MIT",
  "homepage": "https://snippetslibrary.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cojocaru-david/snippetslibrary.com"
  },
  "keywords": [
    "snippets",
    "code",
    "library",
    "nextjs",
    "react",
    "drizzle",
    "tailwind",
    "fullstack"
  ],
  "scripts": {
    "dev": "prettier --write . && next dev --turbopack --port 3001",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start --port 3001",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "migrate": "npm run db:generate && npm run db:migrate && npm run db:push",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@hookform/resolvers": "^5.2.0",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/aos": "^3.0.7",
    "@types/uuid": "^10.0.0",
    "aos": "^2.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.44.3",
    "gsap": "^3.13.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.525.0",
    "next": "15.4.4",
    "next-auth": "^4.24.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.18.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "prettier": "^3.6.2",
    "shiki": "^3.8.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
